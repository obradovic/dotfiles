set nocompatible
" set expandtab
set smarttab
set tabstop=4
set shiftwidth=4
set autoindent
" set smartindent
set cindent
set autoindent
set paste

set ru
set sc
set wmnu

set backupdir=/tmp
set dir=/tmp

set incsearch
set hlsearch
set ruler
set showcmd
syntax on
filetype on

